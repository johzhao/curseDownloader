{
    "version": 1,
    "formatters": {
        "simple": {
            "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
        }
    },
    "filters": {
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "level": "DEBUG",
            "formatter": "simple",
            "stream": "ext://sys.stdout"
        },
        "fileHandler": {
            "class": "logging.handlers.RotatingFileHandler",
            "level": "DEBUG",
            "formatter": "simple",
            "filename": "./download.log",
            "maxBytes": 10485760,
            "backupCount": 5,
            "encoding": "utf8"
        }
    },
    "loggers": {
        "simpleExample": {
            "level": "DEBUG",
            "handlers": [
                "console"
            ],
            "propagate": false
        }
    },
    "root": {
        "level": "DEBUG",
        "handlers": [
            "console",
            "fileHandler"
        ]
    },
    "incremental": false,
    "disable_existing_loggers": true
}
